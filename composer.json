{
  "name": "dyrynda/laravel-make-user",
  "description": "An Artisan console command to create users in your Laravel application.",
  "license": "MIT",
  "keywords": [
    "laravel",
    "artisan",
    "console"
  ],
  "authors": [
    {
      "name": "Michael Dyrynda",
      "email": "michael@dyrynda.com.au",
      "homepage": "https://dyrynda.com.au"
    }
  ],
  "require": {
    "php": "^8.2",
    "illuminate/support": "^11.0 || ^12.0",
    "illuminate/console": "^11.0 || ^12.0",
    "illuminate/database": "^11.0 || ^12.0",
    "illuminate/auth": "^11.0 || ^12.0",
    "illuminate/notifications": "^11.0 || ^12.0"
  },
  "autoload": {
    "psr-4": {
      "Dyrynda\\Artisan\\": "src/"
    }
  },
  "require-dev": {
    "mockery/mockery": "^1.4.4",
    "orchestra/testbench": "^9.0 || ^10.0",
    "pestphp/pest": "^2.0 || ^3.0"
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "laravel": {
      "providers": [
        "Dyrynda\\Artisan\\MakeUserServiceProvider"
      ]
    }
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
